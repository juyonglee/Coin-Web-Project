<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<link rel="stylesheet" type="text/css" href="/stylesheets/manager.css" />
<!------ Include the above in your HEAD tag ---------->

<div class="container">
    <div class="col-sm-2">

        <!--TODO: JUNGEE: 왜 이동안되는지 파악 후, 수정 -->

        <form method="GET" action="/manager/member">
            <input type="submit" value="회원정보"></input>
        </form>

        <form method="GET" action="/manager/deposit">
            <input type="submit" value="임금대기" ></input>
        </form>
        <form method="GET" action="/manager/sales">
            <input type="submit" value="판매완료내역" ></input>
        </form>
        <form method="GET" action="/manager/addsales">
            <input type="submit" value="판매누적액" ></input>
        </form>
        <form method="GET" action="/manager/managermenu">
            <input type="submit" value="관리자 메뉴" ></input>
        </form>



    </div>
    <div class="col-sm-8">
    <!-- tab content -->
    <div class="tab-content">
        <div class="tab-pane active text-style">
            <h2>판매완료 내역 </h2>
            <table class="table">
               <tr>
                   <th>입금날짜</th>
                   <th>회원넘</th>
                   <th>이름</th>
                   <th>종류</th>
                   <th>수량</th>
                   <th>금액</th>
                   <th>처리상태</th>
               </tr>
               <% for(var i=0; i<sales.length; i++) {%>
                <tr>
                    <td><%= sales[i].buy_date %></td>
                    <td><%= sales[i].buyer.sequence %></td>
                    <td><%= sales[i].buyer.name %></td>
                    <td><%= sales[i].coin_name %></td>
                    <td><%= sales[i].buy_count %></td>
                    <td><%= sales[i].price %></td>
                    <td>입금완료</td>
                </tr>
                <% } %>
            </table>

        </div>

    </div>
    </div>


</div>